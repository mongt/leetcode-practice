var twoSum = function(nums, target){

    // start a loop
    for(let i = 0; i < nums.length; i++)    // i = 0
    {
        for(let j = i + 1; j < nums.length; j++)  // j = i + 1 = 0 + 1
        {
            if(nums[i] + nums[j] == target)    // if nums[i] + nums[j] == target 
            {
                result = [i,j];
                return result;
                break;
            }
        }
    }
}
